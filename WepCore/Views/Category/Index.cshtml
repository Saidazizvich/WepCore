@model IEnumerable<WepCore.Models.Entities.Category>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css" integrity="sha384-xOolHFLEh07PJGoPkLv1IbcEPTNtaed2xpHsD9ESMhqIYd0nLMwNLD69Npy4HI+N" crossorigin="anonymous">
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Descripation</th>c
            <th>CreateDate</th>c
            <th>Modefiled</th>
            <th>DeleteDate</th>
            <th>DataStatus</th>
            <th>Delete</th>
            <th>Update</th>
        </tr>
    </thead>
    <tbody>
         @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.CategoryName</td>
                <td>@item.Descripation</td>
                <td>@item.CreatedDate</td>
                <td>@item.ModefiedDate</td>
                <td>@item.DeletedDate</td>
                <td>@item.DataStatus</td>
                <td><a asp-action="Delete" asp-route-id="@item.Id" class="btn btn-danger">Delete</a></td>
                <td><a asp-action="Update" asp-route-id="@item.Id" class="btn btn-warning">Update</a></td>
            </tr>
        }
    </tbody>
</table>

  <a asp-action="GetCategory" class="btn btn-primary">Cretae Category</a>            